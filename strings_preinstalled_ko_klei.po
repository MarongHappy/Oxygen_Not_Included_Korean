#. STRINGS.ELEMENTS.LIQUIDPROPANE.NAME
msgctxt "STRINGS.ELEMENTS.LIQUIDPROPANE.NAME"
msgid "<link=\"LIQUIDPROPANE\">Propane</link>"
msgstr "<link=\"LIQUIDPROPANE\">액화 프로판</link>"

